{
    "$schema": "https://schemas.wp.org/trunk/block.json",
    "apiVersion": 3,
    "version": "0.1.2",
    "name": "multistore/product",
    "title": "Simple Product",
    "category": "widgets",
    "keywords": ["product"],
	"attributes": {
        "postId": {
            "type": "number",
            "default": 0
        },
        "postType": {
            "type": "string",
            "default": "product"
        },
        "isLink": {
            "type": "boolean",
            "default": true
        }
	},
	"supports": {
        "interactivity": true,
        "align": [ "wide", "full" ],
        "anchor": true,
        "customClassName": true,
        "color": {
            "background": true,
            "gradients": true
        },
        "background": {
            "backgroundImage": true,
            "backgroundSize": true
        },
        "spacing": {
            "padding": true,
            "blockGap": true
        }
    },
    "providesContext": {
        "postId": "postId",
        "postType": "postType"
    },
	"textdomain": "multistore",
    "editorScript": "file:./index.js",
    "editorStyle": "file:./index.css",
    "style": "file:./style-index.css",
    "viewScript": "file:./view.js",
    "render": "file:./render.php"
}