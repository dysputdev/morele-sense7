{
	"$schema": "https://schemas.wp.org/wp/6.7/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"color": {
			"custom": true,
			"customDuotone": true,
			"customGradient": true,
			"defaultGradients": false,
			"defaultPalette": false,
			"duotone": [],
			"gradients": [],
			"palette": [
				{ "name": "Base", "slug": "base", "color": "hsl(0, 0%, 100%)" },
				{ "name": "Contrast", "slug": "contrast", "color": "hsl(0, 0%, 15%)" },
				{ "name": "Contrast 80%", "slug": "contrast-80", "color": "hsla(0, 0%, 15%, 0.8)" },
				{ "name": "Contrast 75%", "slug": "contrast-75", "color": "hsla(0, 0%, 15%, 0.75)" },
				{ "name": "Contrast 60%", "slug": "contrast-60", "color": "hsla(0, 0%, 15%, 0.6)" },
				{ "name": "Contrast 40%", "slug": "contrast-40", "color": "hsla(0, 0%, 15%, 0.4)" },

				{ "name": "Primary", "slug": "primary", "color": "hsl(229, 62%, 46%)" },
				{ "name": "Primary 50%", "slug": "primary-50", "color": "hsla(229, 62%, 46%, 0.5)" },
				{ "name": "Secondary", "slug": "secondary", "color": "hsl(4, 74%, 47%)" },
				{ "name": "Secondary 50%", "slug": "secondary-50", "color": "hsla(4, 74%, 47%, 0.5)" },

				{ "name": "Accent", "slug": "accent", "color": "hsl(0, 0%, 95%)" },
				{ "name": "Accent 2", "slug": "accent-2", "color": "hsl(229, 74%, 61%)" },
				{ "name": "Accent 3", "slug": "accent-3", "color": "hsl(4, 74%, 58%)" },
				{ "name": "Accent 4", "slug": "accent-4", "color": "hsl(38, 88%, 60%)" },

				{ "name": "Black", "slug": "black", "color": "hsla(0, 0%, 0%, 1)" },
				{ "name": "Gray 80%", "slug": "gray-80", "color": "hsla(0, 0%, 80%, 1)" },
				{ "name": "Gray 60%", "slug": "gray-60", "color": "hsla(0, 0%, 60%, 1)" },
				{ "name": "Gray 50%", "slug": "gray-50", "color": "hsla(0, 0%, 50%, 1)" },
				{ "name": "Gray 40%", "slug": "gray-40", "color": "hsla(0, 0%, 40%, 1)" },
				{ "name": "Gray 30%", "slug": "gray-30", "color": "hsla(0, 0%, 30%, 1)" },
				{ "name": "Gray 20%", "slug": "gray-20", "color": "hsla(0, 0%, 20%, 1)" },
				{ "name": "Gray 15%", "slug": "gray-15", "color": "hsla(0, 0%, 15%, 1)" }
			]
		},
		"layout": {
			"contentSize": "1200px",
			"wideSize": "1648px"
		},
		"spacing": {
			"defaultSpacingSizes": false,
			"spacingSizes": [
				{
					"slug": "20",
					"size": "0.25rem",
					"name": "Extra small"
				},
				{
					"slug": "30",
					"size": "0.5rem",
					"name": "Small"
				},
				{
					"slug": "40",
					"size": "1rem",
					"name": "Normal"
				},
				{
					"slug": "50",
					"size": "1.5rem",
					"name": "Medium"
				},
				{
					"slug": "60",
					"size": "2rem",
					"name": "Large"
				},
				{
					"slug": "70",
					"size": "3rem",
					"name": "X-Large"
				},
				{
					"slug": "80",
					"size": "4rem",
					"name": "XXL"
				},
				{
					"slug": "90",
					"size": "5rem",
					"name": "3XL"
				}
			],
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			]
		},
		"typography": {
			"writingMode": true,
			"defaultFontSizes": false,
			"fluid": true,
			"fontSizes": [
				{
					"slug": "small",
					"size": "0.875rem",
					"name": "Small (14px)"
				},
				{
					"slug": "base",
					"size": "1rem",
					"name": "Base (16px)"
				},
				{
					"slug": "medium",
					"size": "1.25rem",
					"name": "Medium (20px)"
				},
				{
					"slug": "large",
					"size": "1.5rem",
					"name": "Large (24px)"
				},
				{
					"slug": "x-large",
					"size": "2rem",
					"name": "Extra Large (32px)"
				},
				{
					"slug": "xx-large",
					"size": "2.5rem",
					"name": "2X Large (40px)"
				},
				{
					"slug": "xxx-large",
					"size": "3.5rem",
					"name": "3X Large (48px)"
				}
			],
			"fontFamilies": [
				{
					"name": "Sense7",
					"slug": "brand",
