								"file:./assets/fonts/Avenir/subset-Avenir-Book.woff",
								"file:./assets/fonts/Avenir/subset-Avenir-Book.ttf",
								"file:./assets/fonts/Avenir/subset-Avenir-Book.eot",
								"file:./assets/fonts/Avenir/subset-Avenir-Book.svg"
							]
						},
						{
							"fontFamily": "Sense7",
							"fontWeight": "900",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Avenir/subset-Avenir-HeavyOblique.woff2",
								"file:./assets/fonts/Avenir/subset-Avenir-HeavyOblique.woff",
								"file:./assets/fonts/Avenir/subset-Avenir-HeavyOblique.ttf",
								"file:./assets/fonts/Avenir/subset-Avenir-HeavyOblique.eot",
								"file:./assets/fonts/Avenir/subset-Avenir-HeavyOblique.svg"
							]
						},
						{
							"fontFamily": "Sense7",
							"fontWeight": "normal",
							"fontStyle": "italic",
							"fontDisplay": "swap",
							"src": [
								"file:./assets/fonts/Avenir/subset-Avenir-BookOblique.woff2",
								"file:./assets/fonts/Avenir/subset-Avenir-BookOblique.woff",
								"file:./assets/fonts/Avenir/subset-Avenir-BookOblique.ttf",
								"file:./assets/fonts/Avenir/subset-Avenir-BookOblique.eot",
								"file:./assets/fonts/Avenir/subset-Avenir-BookOblique.svg"
